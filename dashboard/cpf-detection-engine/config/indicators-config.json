{
  "indicators": {
    "1.1": {
      "id": "1.1",
      "name": "Unquestioning Compliance",
      "category": "Authority",
      "category_id": 1,
      "description": "Monitors compliance patterns with perceived authority through analysis of authentication logs, email headers, and approval chains",
      "observables": [
        "complianceRate",
        "verificationRate",
        "responseTime"
      ],
      "dataSources": [
        "emails",
        "approvals",
        "financial_transactions"
      ],
      "temporal": {
        "window_seconds": 3600,
        "persistence_tau": 14400
      },
      "detection": {
        "threshold_k": 2.0,
        "weights": {
          "w1": 0.4,
          "w2": 0.4,
          "w3": 0.2
        }
      },
      "baseline_defaults": {
        "compliance_rate": {
          "mean": 0.72,
          "std": 0.08
        },
        "verification_rate": {
          "mean": 0.35,
          "std": 0.12
        },
        "response_time": {
          "mean": 300,
          "std": 120
        }
      }
    },
    "1.2": {
      "id": "1.2",
      "name": "Diffusion of Responsibility",
      "category": "Authority",
      "category_id": 1,
      "description": "Tracks ticket ownership transitions where T_ownership > 3 indicates diffusion",
      "observables": [
        "ownershipTransitions",
        "transitionTime",
        "workTime"
      ],
      "dataSources": [
        "tickets",
        "incidents"
      ],
      "temporal": {
        "window_seconds": 86400,
        "persistence_tau": 14400
      },
      "detection": {
        "threshold_k": 2.0,
        "weights": {
          "w1": 0.3,
          "w2": 0.5,
          "w3": 0.2
        }
      }
    },
    "2.1": {
      "id": "2.1",
      "name": "Urgency-Induced Bypass",
      "category": "Temporal",
      "category_id": 2,
      "description": "Quantifies through Ui = (Δt_normal - Δt_urgent) / Δt_normal where Δt represents task completion time",
      "observables": [
        "accelerationIndex",
        "bypassRate",
        "pressureScore"
      ],
      "dataSources": [
        "tasks",
        "approvals",
        "policy_exceptions"
      ],
      "temporal": {
        "window_seconds": 86400,
        "persistence_tau": 1800
      },
      "detection": {
        "threshold_k": 2.0,
        "weights": {
          "w1": 0.4,
          "w2": 0.4,
          "w3": 0.2
        }
      }
    },
    "5.1": {
      "id": "5.1",
      "name": "Alert Fatigue",
      "category": "Cognitive Overload",
      "category_id": 5,
      "description": "Operationalizes as Fa = 1 - investigated/presented with temporal decay modeling",
      "observables": [
        "fatigueIndex",
        "decayRate",
        "criticalIgnoredRate"
      ],
      "dataSources": [
        "security_alerts"
      ],
      "temporal": {
        "window_seconds": 86400,
        "persistence_tau": 86400
      },
      "detection": {
        "threshold_k": 2.0,
        "weights": {
          "w1": 0.5,
          "w2": 0.3,
          "w3": 0.2
        }
      },
      "critical_threshold": {
        "fatigue_index": 0.7,
        "critical_ignored": 0.1
      }
    }
  },
