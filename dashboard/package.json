{
  "name": "cpf-dashboard-server",
  "version": "1.0.0",
  "description": "CPF Dashboard Server - Unified server for SOC Dashboard, Auditing Dashboard, and Field Kit Client",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "generate:demo": "node scripts/generate_demo_organizations.js",
    "db:schema:postgres": "psql cpf_db -f lib/schemas/db_schema_postgres.sql",
    "db:schema:sqlite": "sqlite3 cpf.db < lib/schemas/db_schema_sqlite.sql",
    "db:seed": "node postgres/db_seed_demo.js",
    "db:setup:postgres": "createdb cpf_db && npm run db:schema:postgres && npm run db:seed",
    "db:setup:sqlite": "npm run db:schema:sqlite && npm run db:seed",
    "test": "node test_backend.js"
  },
  "keywords": [
    "cpf",
    "dashboard",
    "bayesian",
    "security",
    "field-kit"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "socket.io": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  }
}
